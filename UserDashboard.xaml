<Window x:Class="TicketTest.UserDashboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TicketTest"
        mc:Ignorable="d"
        Title="UserDashboard" Height="392" Width="428">
    <Grid>
        <Label x:Name="lblName" Content="Hi! What would you like to do?" HorizontalAlignment="Center" Margin="0,37,0,0" VerticalAlignment="Top" FontSize="20" HorizontalContentAlignment="Center"/>
        <Button x:Name="btnProfile" Content="View Profile" HorizontalAlignment="Center" Margin="0,130,0,0" VerticalAlignment="Top" Height="53" Width="156" FontSize="20"/>
        <Button x:Name="btnSearchTicket" Content="Search Ticket" HorizontalAlignment="Center" Margin="0,244,0,0" VerticalAlignment="Top" Height="53" Width="156" FontSize="20" Click="btnSearchTicket_Click"/>

    </Grid>
</Window>
